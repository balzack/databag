          required: true
          schema:
            type: string
        - name: revision
          in: query
          description: return updated topics since revision 
          required: false
          schema:
            type: integer
            format: int64
        - name: count
          in: query
          description: limit number of latest from latest when revision not set
          required: false
          schema:
            type: integer
            format: int64
        - name: begin
          in: query
          description: return tags after and including sequence marker
          required: false
          schema:
